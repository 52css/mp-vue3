{
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "ES2020",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictPropertyInitialization": true,
    "lib": ["ES2020"],
    "typeRoots": [
      "./typings"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": ["miniprogram/*"] ,
      "tdesign-miniprogram/*":["./miniprogram/miniprogram_npm/tdesign-miniprogram/*"]
    },
  },
  "include": [
    "./**/*.ts"
, "miniprogram/utils/vue.js", "miniprogram/components/product-card/product-card.ts", "miniprogram/pages/index/index.ts", "miniprogram/pages/login/login.ts", "miniprogram/utils/network.js", "miniprogram/pages/hands-on/test.js", "miniprogram/pages/demo/demo.ts", "miniprogram/components/reactive/demo.js", "miniprogram/components/classin-theory-card/classin-tyeory-card.js", "miniprogram/pages/theory-info/theory-info.ts", "miniprogram/pages/hands-result/hands-result.ts", "miniprogram/components/table/table.ts", "miniprogram/components/table-cell/table-cell.js", "miniprogram/components/table-cell/table-cell.js", "miniprogram/pages/demo/components/cell/demo-table-cell.js", "miniprogram/pages/demo/components/cell/cell.ts", "miniprogram/pages/theory-result/theory-result.ts", "miniprogram/pages/wrong-list/wrong-list.ts", "miniprogram/components/wrong-card/wrong-card.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
